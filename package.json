{
	"name": "playwright-js-automation-testing",
	"version": "1.0.0",
	"description": "Playwright JS Automation Testing",
	"main": "index.js",
	"scripts": {
		"regressionTests": "npx playwright test",
		"ecommerceTests": "npx playwright test --grep @Ecommerce",
		"apiTests": "npx playwright test --grep @API",
		"safariNewConfig": "npx playwright test --config playwright.config1.js --project=safari",
		"cucumberRegressionTests": "npx cucumber-js --tags '@Regression' --retry 1 --exit --format html:cucumber-report.html"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@cucumber/cucumber": "^10.4.0",
		"@playwright/test": "^1.43.1",
		"@types/node": "^20.11.28",
		"allure-playwright": "^2.15.1"
	},
	"dependencies": {
		"allure-commandline": "^2.28.0",
		"dotenv": "^16.4.5",
		"exceljs": "^4.4.0",
		"playwright-chromium": "^1.43.1"
	}
}
